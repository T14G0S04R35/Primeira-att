programa {
  funcao inicio() {
    inteiro numero 

    escreva("Digite seu numero: ")
    leia(numero)

    se (numero == 10 ) {
      escreva("É igual a 10. ")
    } senao {

      se (numero)
      escreva("é maior que 10. ")
    }

    se(numero > 10) {
      escreva("Nao é maior que 10.")
    }
  }
}
